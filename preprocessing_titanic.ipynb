   "metadata": {},
   "outputs": [],
   "source": [
    "#fill missing values\n",
    "df['Age'].fillna(df['Age'].median(), inplace=True)\n",
    "# First check if 'Embarked' exists in df\n"
    "if 'Embarked' in df.columns:\n"
    "    df['Embarked'] = df['Embarked'].fillna(df['Embarked'].mode()[0])\n"
    "else\n"
    "    print("Column 'Embarked' not found! You might have already encoded it.")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 55,
   "id": "2698640b-00aa-4087-b2c6-4bd119a7e708",
   "metadata": {},
   "outputs": [],
   "source": [
    "#encode categorial columns\n",
    "df['Sex'] = df['Sex'].map({'male': 0, 'female': 1})\n",
    "df = pd.get_dummies(df, columns=['Embarked'], drop_first=True)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 56,
   "id": "a2fb99cd-7ca9-47bd-8c1c-650f9aebfe5b",
   "metadata": {},
   "outputs": [],
   "source": [
    "#create family feature\n",
    "df['Family'] = df['SibSp'] + df['Parch'] + 1\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 57,
   "id": "5c1f9b4c-408b-4de8-a351-71cabc45140f",
   "metadata": {},
   "outputs": [],
   "source": [
    "#feature scaling\n",
    "scaler = StandardScaler()\n",
    "df[['Age', 'Fare']] = scaler.fit_transform(df[['Age', 'Fare']])\n"
   ]
